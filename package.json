{
  "name": "vas",
  "version": "2.1.0",
  "description": "simple composable data services using muxrpc",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test bin/test",
    "test:coverage": "NODE_ENV=test istanbul cover ./test.js",
    "test": "npm-run-all -s test:deps test:lint test:node"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/vas.git"
  },
  "keywords": [
    "simple",
    "composable",
    "data",
    "service",
    "micro",
    "muxrpc",
    "stream",
    "pull",
    "pull-stream"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/vas/issues"
  },
  "homepage": "https://github.com/ahdinosaur/vas#readme",
  "devDependencies": {
    "bulk-require": "^1.0.0",
    "dependency-check": "^2.6.0",
    "istanbul": "^0.4.5",
    "mock-xmlhttprequest": "github:ahdinosaur/mock-xmlhttprequest",
    "npm-run-all": "^1.8.0",
    "object-values": "^1.0.0",
    "standard": "^7.1.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "aspects": "^1.0.2",
    "boom": "^4.2.0",
    "deep-assign": "^2.0.0",
    "depject": "github:pietgeursen/depject#refactor",
    "es2040": "^1.2.4",
    "explain-error": "^1.0.3",
    "fast-json-parse": "^1.0.2",
    "fast-safe-stringify": "^1.1.3",
    "from2-string": "^1.1.0",
    "is-pull-stream": "0.0.0",
    "is-stream": "^1.1.0",
    "libnested": "^1.1.0",
    "object-assign": "^4.1.0",
    "pino": "^3.1.1",
    "pull-stream": "^3.5.0",
    "pull-stream-to-stream": "^1.3.3",
    "pump": "^1.0.2",
    "query-string": "^4.3.1",
    "server-sink": "^1.0.0"
  }
}
