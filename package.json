{
  "name": "vas",
  "version": "2.1.0",
  "description": "simple composable data services using muxrpc",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test tape test/*.js",
    "test:coverage": "NODE_ENV=test istanbul cover test",
    "test": "npm-run-all -s test:deps test:lint test:node"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/vas.git"
  },
  "keywords": [
    "simple",
    "composable",
    "data",
    "service",
    "micro",
    "muxrpc",
    "stream",
    "pull",
    "pull-stream"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/vas/issues"
  },
  "homepage": "https://github.com/ahdinosaur/vas#readme",
  "devDependencies": {
    "dependency-check": "^2.5.1",
    "istanbul": "^0.4.4",
    "npm-run-all": "^1.6.0",
    "standard": "^7.1.2",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "aspects": "^1.0.2",
    "es2040": "^1.2.3",
    "get-in": "^3.0.1",
    "pull-defer": "^0.2.2",
    "pull-pushable": "^2.0.1",
    "pull-stream": "^3.5.0",
    "set-in": "^2.0.0",
    "uuid": "^3.0.1"
  }
}
