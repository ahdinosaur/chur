{
  "name": "vas",
  "version": "2.1.0",
  "description": "simple composable data services using muxrpc",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "example": "node-dev example",
    "test:deps": "dependency-check . && dependency-check . --extra --no-dev -i es2040",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test bin/test",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "NODE_ENV=test nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:deps test:lint test:node"
  },
  "browserify": {
    "transform": [
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/vas.git"
  },
  "keywords": [
    "simple",
    "composable",
    "data",
    "service",
    "micro",
    "muxrpc",
    "stream",
    "pull",
    "pull-stream"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/vas/issues"
  },
  "homepage": "https://github.com/ahdinosaur/vas#readme",
  "devDependencies": {
    "bulk-require": "^1.0.0",
    "dependency-check": "^2.6.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^1.8.0",
    "nyc": "^10.1.2",
    "object-values": "^1.0.0",
    "standard": "^7.1.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "aspects": "^1.0.2",
    "deep-assign": "^2.0.0",
    "depject": "^3.1.4",
    "es2040": "^1.2.4",
    "libnested": "^1.1.0"
  }
}
